{
  "name": "cywidget-table",
  "version": "0.0.1",
  "description": "Table viewer widget using React.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --content-base build",
    "build": "webpack --progress --colors",
    "clean": "rm build/widget.js",
    "lint": "eslint src/; exit 0",
    "test": "eslint src/ && jest",
    "coverage": "jest --coverage"
  },
  "main": "build/widget.js",
  "jest": {
    "scriptPreprocessor": "<rootDir>/.preprocessor.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cytoscape-ci/cywidget-table.git"
  },
  "keywords": [
    "Bioinformatics",
    "cytoscape"
  ],
  "author": "Keiichiro Ono",
  "contributors": [
    {
      "name": "Eric Sage",
      "email": "eric.david.sage@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cytoscape-ci/cywidget-table/issues"
  },
  "homepage": "https://github.com/cytoscape-ci/cywidget-table",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "es6": true,
      "browser": true
    }
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "files": [
    "build",
    "src"
  ],
  "dependencies": {
    "cywidget-network": "0.0.30",
    "es6-promise": "^3.0.2",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.14.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router-redux": "^3.0.0",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1",
    "redux-logger": "^2.5.2",
    "redux-thunk": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "css-loader": "^0.23.0",
    "eslint": "^2.0.0",
    "file-loader": "^0.8.5",
    "jest-cli": "^0.8.2",
    "jest-webpack-alias": "^2.0.0",
    "node-sass": "^3.4.2",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.0"
  }
}
